{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-console": "warn",
    "prefer-const": "warn",
    "quotes": ["error", "single"],
    "indent": ["warn", 2],
    "max-len": ["warning", {"code": 120}],
    "jsx-quotes": ["warning", "prefer-double"],
    "comma-dangle": ["error", "always-multiline"],
    "semi": ["warn", "always"]
  }
}